{
	"name": "{{kebab name}}",
	"version": "0.0.0",
	"description": "{{description}}",
	"author": {
		"name": "{{author}}",
		"email": "{{email}}"
	},
	"license": "UNLICENSED",
	"scripts": {
		"build": "rimraf dist && cross-env NODE_ENV=production webpack",
		"coverage": "cross-env NODE_ENV=test nyc mocha",
		"format": "prettier --check {src,tests}/**",
		"lint": "eslint \"src/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}\"",
		"serve": "cross-env NODE_ENV=development webpack serve --devtool=source-map",
		"test": "cross-env NODE_ENV=test mocha",
		"postinstall": "npm-run-all postinstall:*",
		"postinstall:components-assets": "cpy \"node_modules/@public-ui/components/assets/**/*\" public/assets --dot",
		"postinstall:themes-assets": "cpy \"node_modules/@public-ui/themes/assets/**/*\" public/assets --dot",
		"update": "ncu -t minor -u && ncu"
	},
	"dependencies": {
		"@public-ui/components": "1.6.0-rc.25",
		"@public-ui/themes": "1.6.0-rc.25",
		"@public-ui/vue": "1.6.0-rc.25",
		"vue": "3.3.4"
	},
	"devDependencies": {
		"@leanup/stack": "1.3.41",
		"@leanup/stack-webpack": "1.3.41",
		"@leanup/stack-vue3": "1.3.41",
		"@unocss/preset-mini": "0.45.29",
		"@unocss/webpack": "0.45.29",
		"chromedriver": "114.0.3",
		"cpy-cli": "5.0.0",
		"npm-check-updates": "16.6.2",
		"npm-run-all": "4.1.5",
		"typescript": "4.9.4"
	},
	"files": [
		"dist"
	]
}
